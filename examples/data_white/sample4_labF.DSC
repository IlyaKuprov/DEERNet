                ; time scale increment of dipolar evolution time\n\
                \n\
d31  = 16      ; 2nd time scale increment\n\
                ; for nuclear modulation\n\
                ; for protons (don't use in Q-band!)    8 ns\n\
                ; for deuterons                        16 ns\n\
\n\
a    = 20      ; (RESERVED) number of transient averages (TRANS)\n\
               ; if > than 1024, a must be a multiple of 1024\n\
h    = 5      ; (CONVENTION) shots per point (INTG and SPT)\n\
n    = 1       ; (CONVENTION) number of sweeps to accumulate\n\
               ; (counter: K)                  100 000\n\
\n\
m    = 8       ; number of tau1 averages for nuclear modulation\n\
               ; suppression, \n\
                    ; Q band: proton suppression not necessary(m = 1)\n\
               ; for deuterium: use m = 8 and d31 = 16\n\
\n\
\n\
srt  = 5000 * srtu ; (RESERVED) SRT - Shot Repetition Time (srtu=1.02 us)\n\
\n\
dx   = 0       ; (RESERVED) 1st time scale starting value\n\
dy   = 0       ; (RESERVED) 2nd time scale starting value\n\
\n\
end defs\n\
