; AWG_def_4P-DEER\n\
; last modified: ST, 27/02/2018\n\
;\n\
; no safe guards are present to prevent ELDOR pulse from overlapping Probe Pulse\n\
; dim must be adapted to delays! \n\
; sx = ( d1 + d2 - 2 * d3 )/d30 for p3>d3.\n\
; sx = (d1+ d2 - p3 - d3)/d30 for p3>d3.\n\
;\n\
begin defs			; dimension of data array [sx,sy]\n\
 dim s[1024]		; for DEER Setup 	[TRANS QUAD]\n\
 dim1 s[890]		; for standing DEER	[INTG QUAD]\n\
 dim2 s[1010]		; for 1D 4-Pulse DEER\n\
 dim3 s[730]		; for 1D 4-Pulse DEER + tau averaging\n\
 dim4 s[605,300]		; for 2D DEER\n\
end defs\n\
;\n\
;\n\
; define awg channels to use for different phase cycles\n\
;\n\
; phase cycling of observer pulse (freq1)\n\
;\n\
begin awg0\n\
    af0               ; frequency start [MHz]\n\
    af1               ; frequency end [MHz]\n\
    0               ; phase [degree]\n\
    aa0               ; amplitude [%]\n\
    as0               ; shape\n\
end awg0\n\
\n\
begin awg1\n\
    af0               ; frequency start [MHz]\n\
    af1               ; frequency end [MHz]\n\
    180               ; phase [degree]\n\
    aa0               ; amplitude [%]\n\
    as0               ; shape\n\
end awg1\n\
\n\
begin awg2\n\
    af0               ; frequency start [MHz]\n\
    af1               ; frequency end [MHz]\n\
     90               ; phase [degree]\n\
    aa0               ; amplitude [%]\n\
    as0               ; shape\n\
end awg2\n\
\n\
begin awg3\n\
    af0               ; frequency start [MHz]\n\
    af1               ; frequency end [MHz]\n\
    270               ; phase [degree]\n\
    aa0               ; amplitude [%]\n\
    as0               ; shape\n\
end awg3\n\
\n\
\n\
; phase cycling of pump pulse (freq2)\n\
;\n\
begin awg4	    ; replaces ELDOR\n\
    af2             ; frequency start [MHz]\n\
    af3             ; frequency end [MHz]\n\
    0               ; phase [degree]\n\
    aa1             ; amplitude [%]\n\
    as1               ; shape\n\
end awg4\n\
\n\
begin awg5	    ; replaces ELDOR\n\
    af2             ; frequency start [MHz]\n\
    af3             ; frequency end [MHz]\n\
    180               ; phase [degree]\n\
    aa1             ; amplitude [%]\n\
    as1               ; shape\n\
end awg5\n\
\n\
begin awg6	    ; replaces ELDOR\n\
    af2             ; frequency start [MHz]\n\
    af3             ; frequency end [MHz]\n\
     90               ; phase [degree]\n\
    aa1             ; amplitude [%]\n\
    as1               ; shape\n\
end awg6\n\
\n\
begin awg7	    ; replaces ELDOR\n\
    af2             ; frequency start [MHz]\n\
    af3             ; frequency end [MHz]\n\
    270               ; phase [degree]\n\
    aa1             ; amplitude [%]\n\
    as1               ; shape\n\
end awg7\n\
\n\
; phase cycles\n\
;\n\
begin lists "none"\n\
 ph1 AWG0\n\
 ph2 AWG0\n\
 ph3 AWG4\n\
 ph4 AWG0\n\
 asg1 +a\n\
 bsg1 +b\n\
end lists\n\
\n\
begin lists1 "2-step"\n\
 ph1 AWG0 AWG1\n\
 ph2 AWG0\n\
 ph3 AWG4\n\
 ph4 AWG0\n\
 asg1 +a -a\n\
 bsg1 +b -b \n\
end lists1\n\
\n\
begin lists2 "8-step {(x)[x] xp x}"\n\
 ph1 AWG0 AWG1\n\
 ph2 AWG0 AWG0 AWG1 AWG1 AWG2 AWG2 AWG3 AWG3\n\
 ph3 AWG4\n\
 ph4 AWG0\n\
 asg1 +a -a +a -a -a +a -a +a\n\
 bsg1 +b -b +b -b -b +b -b +b\n\
end lists2\n\
\n\
begin lists3 "8-step on pump {(x) x [xp] x}"\n\
 ph1 AWG0 AWG1\n\
 ph2 AWG0\n\
 ph3 AWG4 AWG4 AWG5 AWG5 AWG6 AWG6 AWG7 AWG7\n\
 ph4 AWG0\n\
 asg1 +a -a +a -a +a -a +a -a \n\
 bsg1 +b -b +b -b +b -b +b -b\n\
end lists3\n\
\n\
begin lists4 "8-step on observer {[x] (x) xp x}"\n\
 ph1 AWG0 AWG0 AWG1 AWG1 AWG2 AWG2 AWG3 AWG3\n\
 ph2 AWG0 AWG1\n\
 ph3 AWG4\n\
 ph4 AWG0\n\
 asg1 +a +a -a -a +b +b -b -b\n\
 bsg1 +b +b -b -b -a -a +a +a\n\
end lists4\n\
\n\
;\n\
; 4-Pulse ELDOR Set-Up\n\
;\n\
begin exp "4P DEER Setup" [TRANS QUAD]\n\
\n\
d7=d1+d2			; calculate seperation\n\
d7=d7-d3\n\
\n\
    shot i=1 to a	; accumulation loop\n\
      d9			; DAF (Delay After Flash)\n\
      p0 [ph1]		; 1st observer pulse\n\
      d1			; tau1\n\
      p1 [ph2]		; 2nd observer pulse\n\
      d3			; T + d30 * sx < d1 + d2\n\
      p3 [ph3]		; PUMP pulse\n\
      d7			; tau2+tau1-T\n\
      p1 [ph4]		; 3rd observer pulse\n\
      d2			; tau2\n\
      d0			; constant acquisition delay\n\
      dig [sg1]		; acquisition\n\
    next i			; end of accumulation loop\n\
\n\
end exp\n\
\n\
; 4P-standing DEER\n\
; modified script from exp5 "standing DEER" from E1-script \n\
; and PatCarl's awg-script from 05/2017. \n\
; Might contain mistakes!\n\
; last modified: 31/05/2017, ST\n\
;\n\
begin exp1 "standing DEER" [INTG QUAD]\n\
\n\
 for k=1 to n		; averaging loop\n\
 pg=p0;                 ; integration gate, standard 32 ns\n\
 d13=d1\n\
 d4=d2\n\
\n\
 totscans(n)            ; output of total number of scans\n\
 \n\
   sweep x=1 to sx      ; sweep loop\n\
    shot i=1 to h       ; accumulation loop\n\
      d9                ; DAF\n\
      p0 [ph1]          ; 1st pulse and phase program\n\
      d1                ; tau1\n\
      p1 [ph2]          ; 2nd pulse and phase program\n\
      d13               ; tau1 \n\
      p3 [ph3]          ; pi pump pulse on 2nd frequency\n\
      d4                ; tau2\n\
      p1 [ph4]		  ; 3. observer pulse\n\
      d2			  ; tau 2\n\
      d0                ; constant acquisition delay\n\
      acq [sg1]         ; acquisition\n\
    next i              ; end of accumulation loop\n\
    dx=dx+d30           ; define trigger increment   \n\
   next x               ; end of sweep loop\n\
\n\
   dx=0\n\
  \n\
  scansdone(k)          ; output of scans done\n\
 next k\n\
\n\
end exp1  \n\
\n\
;\n\
; 4-Pulse ELDOR\n\
;\n\
begin exp2 "4P DEER"  [INTG QUAD]\n\
\n\
d8=d3				; store T\n\
 for k=1 to n		; averaging loop\n\
   totscans (n)		; output of total number of scans\n\
\n\
   d3=d8			; recall T for averaging loop\n\
   d7=d1+d2		; calculate separation\n\
   d7=d7-d3\n\
   dx=d3-d1		; and define x-axis\n\
\n\
   sweep x=1 to sx	; T sweep loop\n\
    shot i=1 to h		; accumulation loop\n\
      d9			; DAF (Delay After Flash)\n\
      p0 [ph1]		; 1st pulse\n\
      d1			; tau1\n\
      p1 [ph2]		; 2nd pulse\n\
      d3			; T + d30 * sx-1 < d1 + d2\n\
      p3 [ph3]		; ELDOR pulse\n\
      d7			; tau1+tau2-T\n\
      p1 [ph4]		; 3rd pulse\n\
      d2			; tau2\n\
      d0			; constant acquisition delay\n\
      acq [sg1]\n\
    next i			; end of accumulation loop\n\
\n\
    d3=d3+d30		; increment T\n\
    d7=d7-d30		; decrement time between pulse 3 and 4\n\
    dx=dx+d30		; increment x axis\n\
   next x			; end of T sweep loop\n\
\n\
  scansdone(k)		; output number of scans done\n\
 next k\n\
\n\
end exp2\n\
\n\
;\n\
; 4-Pulse ELDOR\n\
;\n\
begin exp3 "4P DEER tau1 avg"  [INTG QUAD]\n\
\n\
d8=d3				; store T\n\
d6 = d1\n\
 for k=1 to n		; averaging loop\n\
   totscans (n)		; output of total number of scans\n\
\n\
   d1 = d6			; recall tau1 for tau avging\n\
\n\
for j = 1 to m \n\
\n\
d5 = j - 1\n\
d5 = d5 *d31\n\
   d3=d8+d5		; recall T for averaging loop\n\
   d7=d1+d2		; calculate separation\n\
   d7=d7-d3\n\
   dx=d3-d1		; and define x-axis\n\
\n\
\n\
   sweep x=1 to sx	; T sweep loop\n\
    shot i=1 to h		; accumulation loop\n\
      d9			; DAF (Delay After Flash)\n\
      p0 [ph1]		; 1st pulse\n\
      d1			; tau1\n\
      p1 [ph2]		; 2nd pulse\n\
      d3			; T + d30 * sx-1 < d1 + d2\n\
      p3 [ph3]		; PUMP pulse\n\
      d7			; tau1+tau2-T\n\
      p1 [ph4]		; 3rd pulse\n\
      d2			; tau2\n\
      d0			; constant acquisition delay\n\
      acq [sg1]\n\
    next i			; end of accumulation loop\n\
\n\
    d3=d3+d30		; increment T\n\
    d7=d7-d30		; decrement time between pulse 3 and 4\n\
    dx=dx+d30		; increment x axis\n\
   next x			; end of T sweep loop\n\
\n\
d1 = d1 + d31\n\
d3 = d3 + d31		; noop\n\
next j\n\
\n\
  scansdone(k)		; output number of scans done\n\
 next k\n\
\n\
end exp3\n\
\n\
; \n\
; 4-Pulse ELDOR\n\
; QUAD detection with Integration, tau1 averaging, 2D experiment\n\
; modified script from exp2 "4P ELDOR tau1 avg" (see above) \n\
; and PatCarl's awg-script from 05/2017. \n\
; Might contain mistakes!\n\
;  \n\
begin exp4 "2D 4P DEER"  [INTG QUAD]\n\
	\n\
d8=d3				; store T\n\
d6 = d1			; store tau 1\n\
 for y=1 to sy		; averaging loop\n\
   totscans (sy)		; output of total number of scans\n\
\n\
   pg = p1			; integrator gate = pulse length of 2nd obs\n\
   d1 = d6			; recall tau1 for tau avging\n\
\n\
for j = 1 to m 		; supressing nuclear modulation \n\
				; through averaging loop j\n\
d5 = j - 1			; counter\n\
d5 = d5 *d31		; delay length of increment tau1\n\
   d3=d8 + d5		; d3: new value: initial + increment length \n\
				; (betweeen 2nd observer and pump pulse)\n\
   d7=d1+d2		; calculate separation tau1+tau2-T\n\
   d7=d7-d3		; (between pump pulse & 3th observer pulse)\n\
   dx=d3-d1		; optional: define x-axis in a way t0 = 0\n\
\n\
\n\
   sweep x=1 to sx	; T sweep loop (tau1 is increased stepwise)\n\
    shot i=1 to h	; accumulation loop\n\
      d9			; DAF (Delay After Flash)\n\
      p0 [ph1]		; 1st pulse, phase: 0  -> 180 \n\
      d1			; tau1\n\
      p1 [ph2]		; 2nd pulse, \n\
				; phase 0  -> 180  -> 90  -> 270 \n\
      d3			; T + d30 * sx-1 < d1 + d2\n\
      p3 [ph3]		; PUMP pulse, phase 0 , freq.: start -> end\n\
      d7			; tau1+tau2-T\n\
      p1 [ph4]		; 3rd pulse, phase 0 \n\
      d2			; tau2\n\
      d0			; constant acquisition delay\n\
      acq [sg1]\n\
    next i			; end of accumulation loop\n\
\n\
    d3=d3+d30		; new delay betw. 2nd observer & pump pulse\n\
    d7=d7-d30		; decrement time after pump pulse\n\
    dx=dx+d30		; increment x axis\n\
   next x			; end of T sweep loop\n\
\n\
d1 = d1 + d31		; increment tau1 for nuclear modulation\n\
; d3 = d3 + d31		; pump pulse delay must be incremented, too\n\
				; this is done via d5-counter, see above\n\
				; therefore, this line is redundant\n\
next j			; end of nucl. mod. averaging loop\n\
\n\
  scansdone(y)		; output number of scans done\n\
 next y\n\
\n\
end exp4\n\
